# ===========================
# LocalLab .gitignore
# ===========================
# This project is open source. This gitignore ensures no personal data,
# experimental results, or sensitive information is committed.

# ===========================
# Java / Maven Backend
# ===========================
backend/target/
backend/*.jar
backend/*.war
backend/*.ear
backend/*.class

# Maven
backend/.mvn/timing.properties
backend/.mvn/wrapper/maven-wrapper.jar
backend/pom.xml.tag
backend/pom.xml.releaseBackup
backend/pom.xml.versionsBackup
backend/pom.xml.next
backend/release.properties

# ===========================
# Spring Boot
# ===========================
backend/application-local.yml
backend/application-local.properties
backend/src/main/resources/application-local.yml
backend/src/main/resources/application-local.properties

# ===========================
# Database Files (H2)
# ===========================
# H2 database files contain experiment results and potentially personal data
data/
backend/data/
*.mv.db
*.trace.db
*.lock.db

# ===========================
# Experiment Results & User Data
# ===========================
# Experiment outputs may contain personal or sensitive information
results/
exports/
output/
*.csv
*.json.export

# Uploaded documents (RAG) may contain personal/proprietary content
uploads/
documents/
backend/uploads/
backend/documents/

# ===========================
# Node / React Frontend
# ===========================
frontend/node_modules/
frontend/dist/
frontend/build/
frontend/.cache/
frontend/.parcel-cache/
node_modules/

# Vite
frontend/.vite/

# npm/yarn/pnpm
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*
.pnpm-store/

# ===========================
# Testing & Coverage
# ===========================
# JaCoCo (Java coverage)
backend/jacoco.exec
backend/target/site/jacoco/

# Vitest / Jest
frontend/coverage/
coverage/

